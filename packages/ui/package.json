{
  "name": "ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "require": "./dist/index.js",
      "import": {
        "default": "./dist/index.js",
        "require": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "node esbuild.config.js",
    "build:tsup": "tsup src/index.js --format esm,cjs --dts --external react",
    "clean": "rm -rf dist",
    "dev": "tsup src/index.js --format esm,cjs --watch --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-presets/jest/node"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "esbuild-css-modules-plugin": "^2.7.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "jest": "^26.6.3",
    "jest-presets": "*",
    "react": "^18.2.0",
    "tsconfig": "*",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "react-dom": "^18.2.0"
  }
}
