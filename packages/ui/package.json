{
  "name": "ui-library",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5000",
    "serve": "node ./server/index",
    "build": "node ./esm-vite.config.js && vite build && node ./server/index",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "esbuild": "^0.19.2",
    "fast-glob": "^3.3.1",
    "glob": "^10.3.3",
    "laravel-vite-plugin": "^0.8.0",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "vite-plugin-dts": "^3.5.2",
    "vite-plugin-lib-inject-css": "^1.2.1",
    "vite-plugin-react-css-modules": "^1.0.0",
    "vite-plugin-vercel": "^0.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^1.0.7",
    "vite": "^3.2.7",
    "vite-plugin-css-auto-import": "^0.5.0",
    "vite-plugin-css-injected-by-js": "1.4.0",
    "vite-plugin-lightningcss": "^0.0.5",
    "vite-plugin-load-css-module": "^0.1.4"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint -c .eslintrc.pre-commit.cjs --fix",
      "prettier --cache --write",
      "git add"
    ],
    "*.{css,md,html,json}": [
      "prettier --cache --write",
      "git add"
    ]
  }
}
